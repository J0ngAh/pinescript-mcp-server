{
  "mcpServers": {
    "pinescript-server": {
      "command": "cmd",
      "args": ["/c", "npx", "pinescript-mcp-server"],
      "env": {
        "NODE_OPTIONS": "--enable-source-maps",
        "PINESCRIPT_WORKSPACE": "%USERPROFILE%\\Documents\\PineScripts",
        "PINESCRIPT_CONFIG": "%APPDATA%\\pinescript\\config.json"
      }
    }
  }
}